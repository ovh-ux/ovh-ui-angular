    "karma-nyan-reporter": "^0.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.4",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.0.5",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "marked": "^0.3.6",
    "minimist": "^1.2.0",
    "ng-annotate": "^1.2.2",
    "ng-annotate-loader": "^0.6.1",
    "opn": "^5.1.0",
    "portscanner": "^2.1.1",
    "postcss-loader": "^2.0.8",
