{
  "name": "ovh-ui-angular",
  "description": "A set of maintainable components for the OVH ecosystem (Angular).",
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "version": "2.13.0",
  "keywords": [
    "angular"
  ],
  "repository": "ovh-ux/ovh-ui-angular",
  "main": "./dist/oui-angular.min.js",
  "browser": "./dist/oui-angular.min.js",
  "module": "packages/oui-angular/src/index.js",
  "scripts": {
    "commit": "npm-scripts-config commit",
    "commitmsg": "npm-scripts-config commitmsg",
    "preversion": "npm run build && git add dist",
    "version": "npm-scripts-config version",
    "postversion": "git push && git push --tags",
    "preview-changelog": "npm-scripts-config preview-changelog",
    "build": "npm run build:clean && npm run build:webpack",
    "build:clean": "rimraf dist",
    "build:webpack": "webpack --progress --colors --config build/webpack.dist.babel.js",
    "build:watch": "webpack --progress --colors --config build/webpack.dist.babel.js --watch",
    "test": "npm run unit:ci",
    "unit": "cross-env NODE_ENV=test babel-node node_modules/karma/bin/karma start build/karma.conf.js --reporters nyan,coverage --single-run",
    "unit:ci": "cross-env NODE_ENV=test babel-node node_modules/karma/bin/karma start build/karma.conf.js --reporters spec,coverage --single-run",
    "unit:watch": "cross-env NODE_ENV=test BABEL_ENV=test node -r babel-register node_modules/karma/bin/karma start build/karma.conf.js --watch"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "escape-string-regexp": "^1.0.5",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "angular": "~1.6.1",
    "angular-aria": "~1.6.1",
    "angular-mocks": "~1.6.1",
    "angular-sanitize": "~1.6.1",
    "autoprefixer": "^8.0.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.18.0",
    "bootstrap": "^3.3.7",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-config-ovh": "^0.1.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.3",
    "flatpickr": "4.4.4",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.26.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-nyan-reporter": "^0.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^2.0.4",
    "less": "^3.0.0",
    "less-loader": "^4.0.5",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "marked": "^0.3.6",
    "minimist": "^1.2.0",
    "ng-annotate": "^1.2.2",
    "ng-annotate-loader": "^0.6.1",
    "npm-scripts-config": "^0.0.2",
    "npm-scripts-conventional-changelog": "^0.1.0",
    "opn": "^5.1.0",
    "portscanner": "^2.1.1",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.5.4",
    "style-loader": "^0.20.1",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.0"
  },
  "peerDependencies": {
    "ovh-ui-kit": "*"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.10.0",
    "yarn": ">= 0.17.0"
  }
}
