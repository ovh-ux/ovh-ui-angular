{
  "name": "ovh-ui-angular",
  "description": "A set of maintainable components for the OVH ecosystem (Angular).",
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "version": "1.1.1",
  "keywords": [
    "angular"
  ],
  "repository": "ovh-ux/ovh-ui-angular",
  "main": "./dist/oui-angular.min.js",
  "browser": "./dist/oui-angular.min.js",
  "module": "packages/oui-angular/src/index.js",
  "scripts": {
    "preversion": "npm build && git add dist",
    "postversion": "git push && git push --tags",
    "build": "npm run build:clean && npm run build:webpack",
    "build:clean": "rimraf dist",
    "build:webpack": "webpack --progress --colors --config build/webpack.dist.babel.js",
    "build:watch": "webpack --progress --colors --config build/webpack.dist.babel.js --watch",
    "karma": "cross-env NODE_ENV=test babel-node node_modules/karma/bin/karma start build/karma.conf.js --reporters nyan,coverage --single-run",
    "karma:watch": "cross-env NODE_ENV=test BABEL_ENV=test node -r babel-register node_modules/karma/bin/karma start build/karma.conf.js --watch"
  },
  "dependencies": {
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "angular": "^1.6.1",
    "angular-mocks": "^1.6.1",
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.18.0",
    "bootstrap": "^3.3.7",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-config-ovh": "^0.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.3",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.26.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
